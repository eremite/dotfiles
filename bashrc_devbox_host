export DOCKER_HOST=tcp://192.168.59.103:2375

alias v='vim'
alias ll='ls -alh'

alias devbox='docker run -it --rm --volumes-from data -v /var/run/docker.sock:/var/run/docker.sock eremite/devbox /bin/bash --login'

alias on='boot2docker up; sleep 2; devbox'
alias off='docker stop $(docker ps -q); boot2docker halt'
